@startuml C4_Elements
!include https://raw.githubusercontent.com/plantuml-stdlib/C4-PlantUML/master/C4_Container.puml


System_Boundary(context,"Context"){
    

    Container(master1,"Master")
    Container(robot1,"Robot")
    Container(motordriver1,"Motor Driver")

    Rel(master1,robot1,"Envia comandos")
    Rel(robot1,master1,"Manda informações")
    Rel(robot1,motordriver1,"Controla")
}




@enduml